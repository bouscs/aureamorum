{
  "compilerOptions": {
    "strict": true,
    "target": "ES2015",
    "useDefineForClassFields": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "jsxImportSource": "repeater14-h",
    "outDir": "./dist",
    "isolatedModules": true,
    "declaration": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"],
  "exclude": []
}
